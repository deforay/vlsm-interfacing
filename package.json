    "karma": "6.4.2",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.3.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "moment": "^2.30.1",
    "node-polyfill-webpack-plugin": "3.0.0",
    "npm-run-all": "4.1.5",
    "playwright": "1.40.1",
    "ts-node": "10.9.2",
    "typescript": ">=5.2 <5.3",
    "wait-on": "7.2.0"
  },
  "engines": {
    "node": ">= 16.14.0 || >= 18.10.0"
  },
  "browserslist": [
    "chrome 114"
